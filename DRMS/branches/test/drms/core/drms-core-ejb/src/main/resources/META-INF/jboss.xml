<?xml version="1.0"?>
<jboss
        xmlns="http://www.jboss.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.jboss.com/xml/ns/javaee
                            http://www.jboss.org/j2ee/schema/jboss_5_0.xsd"
        version="3.0">
    <enterprise-beans>
        <message-driven>
            <ejb-name>EmailMDB</ejb-name>
            <destination-jndi-name>queue/pss2email</destination-jndi-name>
            <resource-ref>
                <res-ref-name>jms/QueueFactory</res-ref-name>
                <jndi-name>java:/JmsXA</jndi-name>
            </resource-ref>
        </message-driven>
        <message-driven>
            <ejb-name>EnvoyEJB</ejb-name>
            <destination-jndi-name>queue/pss2envoy</destination-jndi-name>
            <resource-ref>
                <res-ref-name>jms/QueueFactory</res-ref-name>
                <jndi-name>java:/JmsXA</jndi-name>
            </resource-ref>
        </message-driven>
        <message-driven>
            <ejb-name>PeakChoiceMDB</ejb-name>
            <destination-jndi-name>queue/pss2peakchoice</destination-jndi-name>
            <resource-ref>
                <res-ref-name>jms/QueueFactory</res-ref-name>
                <jndi-name>java:/JmsXA</jndi-name>
            </resource-ref>
        </message-driven>
        <message-driven>
            <ejb-name>HTTPPushMDB</ejb-name>
            <destination-jndi-name>queue/pss2push</destination-jndi-name>
            <resource-ref>
                <res-ref-name>jms/QueueFactory</res-ref-name>
                <jndi-name>java:/JmsXA</jndi-name>
            </resource-ref>
        </message-driven>
        <message-driven>
            <ejb-name>DemandLimitingDispatcherMDB</ejb-name>
            <destination-jndi-name>queue/participantIntervalMeterDataUpdate</destination-jndi-name>
            <resource-ref>
                <res-ref-name>jms/QueueFactory</res-ref-name>
                <jndi-name>java:/JmsXA</jndi-name>
            </resource-ref>
        </message-driven>
         <message-driven>
            <ejb-name>AsynchCallMDB</ejb-name>
            <destination-jndi-name>queue/asynchCallQueue</destination-jndi-name>
            <resource-ref>
                <res-ref-name>jms/QueueFactory</res-ref-name>
                <jndi-name>java:/JmsXA</jndi-name>
            </resource-ref>
        </message-driven>
         <message-driven>
            <ejb-name>ApxDispatchMDB</ejb-name>
            <destination-jndi-name>queue/apxDispatchQueue</destination-jndi-name>
            <resource-ref>
                <res-ref-name>jms/QueueFactory</res-ref-name>
                <jndi-name>java:/JmsXA</jndi-name>
            </resource-ref>
        </message-driven>
    </enterprise-beans>
</jboss>
