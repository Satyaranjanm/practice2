                    <xs:element name="programName" minOccurs="0" type="xs:string"  nillable="true"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="ListOfSeasonConfigs">
            	<xs:sequence>
            		<xs:element name="seasonTable" minOccurs="0" type="tns:AkuaSeasonConfig" maxOccurs="unbounded"></xs:element>
                </xs:sequence>
            </xs:complexType>

</xs:schema>