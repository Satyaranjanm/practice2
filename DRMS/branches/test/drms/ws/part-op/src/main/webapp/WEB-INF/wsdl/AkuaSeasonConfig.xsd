            <xs:complexType name="AkuaSeasonConfig">
            	<xs:sequence>
                    <xs:element name="id" type="xs:string" minOccurs="0" nillable="true" />
                    <xs:element name="name" type="xs:string"/>                    
                    <xs:element name="startDate" minOccurs="0" type="xs:dateTime"  nillable="true"/>
